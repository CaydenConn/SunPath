{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["react", "react-native"],
    "resolveJsonModule": true
  },
  "include": ["**/*.ts", "**/*.tsx", "env.d.ts"],
  "extends": "expo/tsconfig.base",
}
